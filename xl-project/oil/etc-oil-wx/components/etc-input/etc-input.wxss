.etc-input-container{
    width: 100%;  
}

.etc-input-box{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: flex-end;
    box-sizing: border-box;
    padding-bottom: 8rpx;
}

.etc-input{
    flex: 1;
    color: #333333;
    font-size: 30rpx;
}

.etc-input-placeholder{
    color: #999999;
    font-size: 30rpx;
}

.etc-input-type-box{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
}

.etc-input-type-text{
    color: #333333;
    font-size: 30rpx;
    font-weight: 500;
}

.etc-input-type-icon{
    width: 26rpx;
    height: 26rpx;
    margin: 0 8rpx 0 6rpx;
}

.etc-input-separation{
    width: 100%;
    height: 2rpx;
    background-color: #f7f7f7;
}

.etc-input-next{
    width: 11rpx;
    height: 22rpx;
    margin-bottom: 8rpx;
    margin-left: 8rpx;
}