.input-associate{
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    width: 100%;
    height: 80rpx;
}
.input-associate-text{
    display: flex;
    color: rgba(0, 0, 0, 0.85);
    font-size: 28rpx;
    text-align: left;
    font-family: PingFangSC-Regular;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    width: 100%;
    flex: 1;
    margin: 20rpx 30rpx;
}
.input-associate-line{
    width: 100%;
    height: 2rpx;
    background-color: #F2F2F2;
    bottom: 0;
}