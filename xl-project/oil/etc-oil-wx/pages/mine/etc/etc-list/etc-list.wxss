page{
    width: 100vw;
    height: auto;
    background-color: #f8f8f8;
}

.etc-list-container{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
}
.etc-list-container-title-box{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    background-color: #FFFFFF;
    box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(0, 0, 0, 0.05);
    text-align: left;
    padding: 32rpx;
    margin-bottom: 10rpx;
}
.etc-list-container-title{
    font-size: 40rpx;
    width: 100%;
    text-align: left;
    /*font-weight: 600;*/
    color: black;
    box-sizing: border-box;
    font-family: PingFangSC-Medium;
}

.etc-list-container-title-sub{
    color: rgba(0, 0, 0, 0.45);
    font-size: 26rpx;
    text-align: left;
    font-family: PingFangSC-Regular;
    margin-top: 18rpx;
}

.etc-list-container-list{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    margin-bottom: 200rpx;
}

.etc-list-container-list-item{
    width: 660rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
    border-radius: 16rpx;
    margin-top: 24rpx;
    box-shadow: 0 0 10rpx 10rpx #ebebeb;
}

.etc-list-container-list-item-card{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 192rpx;
    line-height: 80rpx;
    padding-left: 40rpx;
    padding-right: 40rpx;
    background:linear-gradient(to right, #35968B, #ACE6C6);
    border-top-left-radius: 16rpx;
    border-top-right-radius: 16rpx;
    color: white;
    font-size: 32rpx;
    letter-spacing: 2rpx;
    box-sizing: border-box;
}
.etc-list-container-list-item-card-no-bank{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 192rpx;
    line-height: 80rpx;
    padding-left: 40rpx;
    padding-right: 40rpx;
    background:linear-gradient(to right, #35968B, #ACE6C6);
    border-radius: 16rpx;
    color: white;
    font-size: 32rpx;
    letter-spacing: 2rpx;
    box-sizing: border-box;
}

.etc-list-container-list-item-row{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    width: 100%;
}

.etc-list-container-list-item-card-name{
    font-size: 36rpx;
    font-weight: 300;
    text-align: right;
    font-family: PingFang-SC-Heavy;
}

.etc-list-container-list-item-card-bank{
    font-size: 28rpx;
    font-weight: 500;
    text-align: left;
    font-family: PingFang-SC-Medium;
}
.etc-list-container-list-item-card-car{
    font-size: 44rpx;
    font-weight: 300;
    text-align: left;
    font-family: PingFangSC-Regular;

}
.etc-list-container-list-item-icon{
    width: 42rpx;
    height: 42rpx;
}
.etc-list-container-list-item-bottom{
    width: 100%;
    background-color: white;
    padding: 20rpx 40rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    box-sizing: border-box;
    border-bottom-left-radius: 16rpx;
    border-bottom-right-radius: 16rpx;
    position: relative;
    left: 0;
    top: 0;
}
.etc-list-container-list-item-bottom-row{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    box-sizing: border-box;
}
.etc-list-item-bank-type{
    width: 42rpx;
    height: 42rpx;
    /*background-color: rgba(213, 37, 41, 1);*/
}
.etc-list-item-bank-info{
    margin-left: 12rpx;
    color: rgba(0, 0, 0, 0.85);
    font-size: 28rpx;
    text-align: left;
    font-family: PingFang-SC-Bold;
}
.etc-list-item-no{
    color: #666666;
    font-size: 30rpx;
}

.etc-list-item-plate{
    color: #666666;
    font-size: 28rpx;
    margin-top: 8rpx;
    margin-left: 54rpx;
}

.etc-list-item-select{
    position: absolute;
    width: 48rpx;
    height: 48rpx;
    right: 28rpx;
    bottom: 28rpx;
}

.etc-list-container-footer{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    z-index: 100;
    background-color: #FFFFFF;
    height: 196rpx;
    box-shadow: 0rpx -4rpx 20rpx 0rpx rgba(0, 0, 0, 0.04);
}

.etc-list-container-footer-text{
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 26rpx;
    margin: 24rpx 0;
    color: rgba(0, 0, 0, 0.45);
    text-align: left;
    font-family: PingFang-SC-Medium;
}
.etc-list-container-footer-icon{
    width: 28rpx;
    height: 28rpx;
    margin-right: 12rpx;
}
.etc-list-container-footer-btn-item{
    width: 630rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-radius: 56rpx;
    color: #ffffff;
    font-size: 32rpx;
}
.etc-list-container-footer-text-bind{
    margin-left: 10rpx;
    text-decoration: underline;
}

/*验证码*/
.verify-container{
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    width: 600rpx;
    height: 350rpx;
    border-radius: 8rpx;
    background-color: rgba(255, 255, 255, 1);
    margin-bottom: 300rpx;
}
.verify-container-close{
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    right: 28rpx;
    top: 28rpx;
}
.verify-container-title{
     color: rgba(0, 0, 0, 0.85);
     font-size: 36rpx;
     text-align: center;
     font-family: PingFangSC-Regular;
     margin-top: 50rpx;
 }
.verify-container-phone-content{
    margin: 40rpx;
}
.verify-container-phone{
    color: rgba(0, 0, 0, 0.45);
    font-size: 26rpx;
    text-align: center;
    font-family: PingFangSC-Regular;
}
.verify-container-time{
    color: rgba(0, 0, 0, 0.35);
    font-size: 24rpx;
    text-align: center;
    font-family: PingFangSC-Regular;
    border-radius: 8rpx;
    background-color: rgba(255, 255, 255, 1);
    border: 2rpx solid rgba(229, 229, 229, 1);
    padding: 5rpx 26rpx;
    margin-left: 10rpx;
}