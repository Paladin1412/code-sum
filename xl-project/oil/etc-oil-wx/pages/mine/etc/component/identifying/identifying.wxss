.identifying-container{
    width: 100vw;
    height: 100vh;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1000;
    box-sizing: border-box;
    background-color: rgba(0, 0, 0, 0.5);
}

.identifying-input-container{
    width: 86%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    background-color: white;
    position: absolute;
    left: 7%;
    bottom: 180rpx;
    border-radius: 2rpx;
}

.identifying-keyboard-container{
    width: 100vw;
    background-color: #e0e0e0;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: flex-start;
    flex-wrap: wrap;
    position: absolute;
    left: 0;
    bottom: 0;
    padding-top: 1px;
}

.identifying-keyboard-item{
    width: 33.2%;
    height:  108rpx;
    background-color: #ffffff;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    margin-bottom: 1px;
}

.identifying-keyboard-item-text{
    font-weight: 400;
    font-size: 50rpx;
    color: black;
}

.identifying-keyboard-delete{
    width: 33.2%;
    height: 108rpx;
    background-color: #e7e6eb;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    font-weight: 600;
    font-size: 40rpx;
    color: black;
}

.identifying-keyboard-delete-icon{
    width: 58rpx;
    max-height: 70rpx;
}

.identifying-keyboard-item-empty{
    width: 33.2%;
    height: 108rpx;
    background-color: #e7e6eb;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    font-weight: 600;
    font-size: 40rpx;
    color: black;
}

.identifying-keyboard-pos{
    -webkit-transform:translateY(100%);
    transform:translateY(100%);
}

.identifying-close{
    width: 28rpx;
    height: 28rpx;
    position: absolute;
    padding: 28rpx;
    left: 0rpx;
    top: 0rpx;
}

.identifying-title{
    font-size: 36rpx;
    color: black;
    margin: 68rpx 0 20rpx 0;
    letter-spacing: 2rpx;
}

.identifying-phone{
    font-size: 30rpx;
    color: #666666;
    letter-spacing: 2rpx;
}

.identifying-count{
    font-size: 28rpx;
    color: #dcdadd;
    letter-spacing: 2rpx;
    margin: 70rpx 0 32rpx 0;
}

.identifying-text-box{
    width: 70%;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    box-sizing: border-box;
    color: #dedcdf;
    font-weight: 600;
    margin-top: 50rpx;
}

.identifying-text-item{
    color: #dedcdf;
    font-weight: 600;
    font-size: 44rpx;
    width: 100%;
    text-align: center;
}

