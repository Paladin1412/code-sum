/* pages/mine/invoice/history/history.wxss */
@import "../../../../templates/mine/invoice/history/history.wxss";

.invoice-history-container{
    display: flex;
    flex-direction: column;
    width: 100%;
}
.invoice-list-box{
    width: 100vw;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
}
.invoice-list-empty-container{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 200rpx;
}
.invoice-list-empty-icon{
    width: 290rpx;
    height: 290rpx;
}
.invoice-list-empty-text{
    margin-top: 40rpx;
    font-size: 30rpx;
    color: #bbbbbb;
}
