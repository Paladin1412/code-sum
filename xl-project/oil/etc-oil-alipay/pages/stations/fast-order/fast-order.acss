}

.fast-order-coupon-container {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    left: 0;
    top: 0;
    padding-top: 20rpx;
    z-index: 100;
}

.fast-order-coupon-box {
    left: 20rpx;
    width: 710rpx;
    height: 101rpx;
    border-radius: 12rpx;
    background-color: rgba(255, 255, 255, 1);
    box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(0, 0, 0, 0.05);
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    box-sizing: border-box;
    padding-top: 20rpx;
}

.fast-order-confirm-box {
    width: 750rpx;
    padding: 30rpx 32rpx;
    background-color: rgba(255, 255, 255, 1);
    box-shadow: 0rpx -4rpx 20rpx 0rpx rgba(0, 0, 0, 0.04);
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    position: fixed;
    bottom: 0px;
}

.fast-order-confirm-hint {
    width: 112rpx;
    color: rgba(0, 0, 0, 0.85);
    font-size: 28rpx;
}

.fast-order-confirm-money-box {
    flex: 1;
    max-height: 52rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
    box-sizing: border-box;
}

.fast-order-confirm-mark {
    color: rgba(5, 186, 125, 1);
    font-size: 24rpx;
    align-self: flex-end;
    padding-bottom: 4rpx;
}

.fast-order-confirm-money {
    color: rgba(5, 186, 125, 1);
    font-size: 40rpx;
    font-weight: 500;
}

.fast-order-confirm-button {
    width: 320rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 32rpx;
}

.fast-order-preferent-container {
    width: 100%;
    height: 870rpx;
    background-color: #f8f8f8;
    border-top-left-radius: 32rpx;
    border-top-right-radius: 32rpx;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
}

.fast-order-preferent-header {
    width: 100%;
    height: 140rpx;
    padding: 0 32rpx;
    background-color: white;
    border-top-left-radius: 32rpx;
    border-top-right-radius: 32rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
}

.fast-order-preferent-header-text {
    font-size: 36rpx;
    color: rgba(0, 0, 0, .85);
    font-weight: 500;
}

.fast-order-preferent-header-image {
    width: 32rpx;
    height: 32rpx;
}

.fast-order-preferent-list {
    width: 100%;
    height: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 20rpx;
    background-color: #f8f8f8;
    box-sizing: border-box;
}