/* pages/station/fast-order/fast-order.acss */

page {
    width: 100%;
    height: auto;
}

.fast-order-box {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    background-color: #f8f8f8;
}

.fast-order-container {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    background-color: #f8f8f8;
    overflow: hidden;
    position: relative;
}

.fast-order-info-container {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    position: relative;
    left: 0;
    top: 0;
    padding-bottom: 180rpx;
}

.fast-order-info-background {
    width: 100%;
    height: 200rpx;
    position: relative;
    left: 0;
    top: 0;
    overflow: hidden;
}

.fast-order-info-background::after {
    content: '';
    width: 140%;
    height: 200rpx;
    position: absolute;
    left: -20%;
    top: 0;
    background-color: #00A170;
    border-radius: 0 0 50% 50%;
}

.fast-order-info-box {
    position: absolute;
    top: 24rpx;
    left: 20rpx;
    width: 710rpx;
    background-color: white;
    border-radius: 12rpx;
    box-shadow: 0 4rpx 20rpx 0 rgba(0, 0, 0, .05);
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    padding: 30rpx 0rpx;
}

.fast-order-info-items {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    box-sizing: border-box;
    width: 100%;
}

.fast-order-info-hint-text {
    left: 50rpx;
    top: 278rpx;
    height: 37rpx;
    color: rgba(0, 0, 0, 0.45);
    font-size: 26rpx;
    text-align: left;
    font-family: PingFangSC-Regular;
    margin: 15rpx 30rpx;
}

.fast-order-info-data-text {
    height: 37rpx;
    color: rgba(0, 0, 0, 0.85);
    font-size: 26rpx;
    text-align: right;
    font-family: PingFangSC-Medium;
    margin: 15rpx 30rpx;
}

.fast-order-discounts-container {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    left: 0;
    top: 0;
    z-index: 100;
}

.fast-order-discounts-box {
    left: 20rpx;
    width: 710rpx;
    height: 277rpx;
    border-radius: 12rpx;
    background-color: rgba(255, 255, 255, 1);
    box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(0, 0, 0, 0.05);
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    align-items: center;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.fast-order-info-items-line {
    left: 50rpx;
    top: 737.5rpx;
    width: 650rpx;
    height: 2rpx;
    background-color: rgba(0, 0, 0, 0.1);
    margin: 15rpx 30rpx;
}

.fast-order-discounts-container {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    left: 0;
    top: 0;
    z-index: 100;
}

