.index-wrapper{
	background-color: rgb(236,241,248);
	width: 100vw;
	height: 100vh;
	overflow: hidden;
}

.scroll-view{
	width: 100vw;
	height: calc(100vh - 100rpx);
	overflow-x: hidden;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
}

.user-info-wrapper{
	width: 100vw;
	height: 126rpx;
	display: flex;
	align-items: center;
	background:url('data:image/png;base64,') no-repeat center center;
	background-size: 100% 100%;
}
.user-wrapper{
	flex: 1;
	height: 100%;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: flex-start;
}

.user-name{
	font-family: SourceHanSansCN-Regular;
	font-size: 36rpx;
	color: white;
	margin-left: 8rpx;
}
.user-cate-wrapper{
	margin-top: 8rpx;
	/*width: 100%;*/
	display: flex;
	margin-right: 10rpx;
}
.user-cate{
	min-width: 166rpx;
	height: 40rpx;
	background-color: rgb(59,97,208);
	font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
	color: rgb(195,207,240);
	border-radius: 20rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 0 20rpx;
}

.avatar-wrapper{
	margin: 0 26rpx;
	height: 100%;
	width: 120rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.avatar{
	width: 96rpx;
	height: 96rpx;
	border-radius: 50%;
}
.login-wrapper{
	flex: 1;
	height: 58rpx;
	display: flex;
	justify-content: flex-start;
}
.login{
	width: 220rpx !important;
	height: 100%;
	border-radius: 28rpx;
	background-color: rgb(80,127,248);
	color: white;
	font-family: SourceHanSansCN-Regular;
	margin: 0 !important;
	font-size: 32rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	border: 0 !important;
}
.right-arrow-wrapper{
	margin-right: 45rpx;
	width: 30rpx;
	height: 100%;
	display: flex;
	justify-content: center;
	align-items: center;
}
.right-arrow{
	width: 14rpx;
	height: 24rpx;
}

.search-wrapper{
	width: 100vw;
	height: 98rpx;
	background-color: rgb(80,127,248);
	display: flex;
	align-items: center;
}

.search-box{
	height: 66rpx;
	width: 686rpx;
	margin: -10rpx 32rpx 0;
	background-color: white;
	border-radius: 33rpx;
	border: 2rpx solid rgb(221,223,231);
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.search-input{
	flex: 1;
	padding-left: 20rpx;
}

input{
	font-family: SourceHanSansCN-Regular;
	font-size: 26rpx;
	color: rgb(173,184,203);
}

.search-icon{
	width: 40rpx;
	height: 40rpx;
	margin: 0 20rpx;
}

.card-guess{
	width: 710rpx;
	margin: 26rpx auto;
	min-height: 50rpx;
	/* padding: 0rpx 22rpx 20rpx; */
}

.category-icon{
	width: 86rpx;
	height: 86rpx;
}
.category-body{
	display: flex;
	flex-wrap: wrap;
	justify-content: space-around;
	align-items: center;
	box-sizing: border-box;
	padding-left: 20rpx;
}
.suggest-service-wrapper{
	width: 100%;
	display: flex;
	align-items: center;
	position: relative;
}
.suggest-mask{
	position: absolute;
	left: 186rpx;
	width: 107rpx;
	height: 40rpx;
}
.service-mask{
	position: absolute;
	right: 186rpx;
	width: 107rpx;
	height: 40rpx;
}
.suggest-service{
	width: 750rpx;
	height: 62rpx;
}
.tab-wrapper{
	width: 750rpx;
	height: 121rpx;
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	z-index: 123;
}
.rel-tab-bar{
	position: relative;
	width: 100%;
	height: 100%;
}
.abs-e{
	position: absolute;
	left: 88rpx;
	height: 98rpx;
	bottom: 0;
	width: 100rpx;
	z-index: 123;
}
.abs-msg{
	position: absolute;
	left: 328rpx;
	height: 98rpx;
	bottom: 0;
	width: 100rpx;
	z-index: 123;
}
.abs-online{
	position: absolute;
	right: 88rpx;
	height: 98rpx;
	bottom: 0;
	width: 100rpx !important;
	z-index: 123;
	background-color:transparent;
	border:none !important;
}
.abs-count{
	position: absolute;
	top: 0;
	left: 380rpx;
	width: 36rpx;
	height: 36rpx;
	background-color: red;
	color: white;
	display: flex;
	justify-content: center;
	align-items: center;
	border-radius: 50%;
	font-size: 28rpx;
	z-index: 223;
}
.tab-bar{
	width: 750rpx;
	height: 121rpx;
}