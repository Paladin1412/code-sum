.userInfoBox {
	width: 750rpx;
	height: 181rpx;
	text-align: center;
	position: relative;
}

.userInfoBgc {
	width: 100%;
	height: 181rpx;
}

.userInfo {
	position: absolute;
	bottom: 70rpx;
	left: 34rpx;
	display: flex;
}


.card_tip {
	font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
	letter-spacing: 0rpx;
	color: #c3cff0;
	width: 161rpx;
	background-color: #3b61d0;
	border-radius: 30rpx;
	padding: 6rpx 17rpx;
}

.body {
	width: 100vw;
	height: calc(100vh - 181rpx);
	background: #f1f3f8;
}

.tips {
	height: 32rpx;
	background: #f1f3f8;
}

.row {
	width: 100vw;
	box-sizing: border-box;
	height: 108rpx;
	padding: 0 30rpx;
	display: flex;
	align-items: center;
	font-size: 30rpx;
	background: #fff;
}

.item-txt {
	flex: 1;
	height: 100%;
	display: flex;
	justify-content: flex-start;
	align-items: center;
	font-family: SourceHanSansCN-Regular;
	font-size: 34rpx;
	line-height: 34rpx;
	letter-spacing: 0rpx;
	color: #3b4257;
}

.item-arrow {
	flex: 0 0 50rpx;
	height: 100%;
	display: flex;
	justify-content: flex-end;
	align-items: center;
}

.user-info-wrapper {
	width: 100vw;
	height: 126rpx;
	display: flex;
	align-items: center;
	background: url('data:image/png;base64,') no-repeat center center;
	background-size: 100% 100%;
}

.avatar-wrapper {
	margin: 0 26rpx;
	height: 100%;
	width: 120rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}

.avatar {
	width: 96rpx;
	height: 96rpx;
	border-radius: 50%;
}

.login-wrapper {
	flex: 1;
	height: 58rpx;
	display: flex;
	justify-content: flex-start;
}

.login {
	width: 220rpx !important;
	height: 100%;
	border-radius: 28rpx;
	background-color: rgb(80, 127, 248);
	color: white;
	font-family: SourceHanSansCN-Regular;
	margin: 0 !important;
	font-size: 32rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	border: 0 !important;
}

.user-wrapper{
	flex: 1;
	height: 100%;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: flex-start;
}

.user-name{
	font-family: SourceHanSansCN-Regular;
	font-size: 36rpx;
	color: white;
	margin-left: 8rpx;
}
.user-cate-wrapper{
	margin-top: 8rpx;
	/*width: 100%;*/
	display: flex;
	margin-right: 10rpx;
}
.user-cate{
	min-width: 166rpx;
	height: 40rpx;
	background-color: rgb(59,97,208);
	font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
	color: rgb(195,207,240);
	border-radius: 20rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 0 20rpx;
}