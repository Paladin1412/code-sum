.wrapper{
  width: 100vw;
  height: 100vh;
  box-sizing: border-box;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  background-color: rgb(241,241,246);
}
.inner-wrapper{
  margin-top: 62rpx;
  margin-left: 24rpx;
  margin-right: 24rpx;
}
.card{
  width: 100%;
  height: 258rpx;
  position: relative;
  margin-bottom: 40rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.card-title{
  width: 100%;
  display: flex;
  box-sizing: border-box;
  padding-top: 52rpx;
  padding-bottom: 70rpx;
  align-items: center;
}
.card-title-icon{
  width: 38rpx;
  height: 28rpx;
  margin-left:46rpx;
  margin-right:14rpx
}
.card-title-text{
  color: white;
  font-family: SourceHanSansCN-Light;
  font-size: 28rpx;
  letter-spacing: 5rpx;
}
.card-title-plate{
  margin-left: 178rpx;
  font-family: SourceHanSansCN-Normal;
  color: white;
  font-size: 40rpx;
}
.card-no{
  font-family: SourceHanSansCN-Normal;
  font-size: 32rpx;
  color: white;
  padding-left: 46rpx;
}
.add-btn{
  width: 100%;
  height: 122rpx;
  font-family: SourceHanSansCN-Light;
  font-size: 32rpx;
  color: rgb(59,66,87);
  border: 2rpx solid rgb(235,235,235);
  border-radius: 10rpx;
  background-color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}
.add-icon{
  width: 38rpx;
  height: 38rpx;
  margin-right: 24rpx;
}