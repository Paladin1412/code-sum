.tiny-skeleton-li {
  margin-top: 16px;
  width: 100%;
  height: 16px;
  list-style: none;
  background: #ccc;
  animation: gradientChange 3s infinite linear alternate;
}

@keyframes gradientChange  {
  50% {
    background: #eee;
    background-position: 0 0;
  }
  100% {
    background: #ccc;
    background-position: 100% 0;
  }
}


