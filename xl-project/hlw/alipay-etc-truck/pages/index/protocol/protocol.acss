.hide {
    display: none !important;
}

text {
    line-height: 150%;
    text-indent: 20rpx;
    text-align: left;
}

.title {
    text-align: center;
    font-size: 28rpx;
    font-weight: bold;
    margin-top: 20rpx;
}

.item-title {
    font-size: 26rpx;
    font-weight: bold;
}

.page {
    padding: 30rpx;
    color: #333;
    font-family: 'PingFang SC', "Arial", "Microsoft YaHei", '黑体', sans-serif;
}

.bottom-line {
    display: inline-block;
    width: 100%;
    border-bottom: 1px solid #ccc;
    margin-bottom: 20rpx;
}

.wrapper {
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.title-content {
    position: relative;
}


