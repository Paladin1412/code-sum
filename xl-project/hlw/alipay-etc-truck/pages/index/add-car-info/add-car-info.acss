.wrapper{
	width: 750rpx;
	box-sizing: border-box;
}
.top-area{
	padding: 30rpx 20rpx;
}
.img-list{
	flex: 0 0 233rpx;
	margin: 20rpx 0;
	display: flex;
	flex-direction: row;
	justify-content: space-around;
}
.img-item{
	position: relative;
	flex: 0 0 323rpx;
	height: 233rpx;
}
.placeholder-img{
	width:100%;
	height:100%;
	position:absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
}
.result-title{
	flex: 0 0 60rpx;
	line-height: 60rpx;
	text-align: left;
	font-size: 26rpx;
	margin: 20rpx 0;
	padding: 0 20rpx;
}
.marital{
	display: flex;
	justify-content: space-between;
	align-items: center;
	height: 100rpx;
	border-bottom: 2rpx solid #eee;
}
.marital-label{
	font-size: 28rpx;
}
.section{
	margin: 10rpx 0;
	border-top:2rpx dashed #eee;
	border-bottom:2rpx dashed #eee;
}
.address{
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.next-btn{
	width: 600rpx;
	margin: 50rpx 75rpx;
	outline: none;
}
.input-item{
	width: 690rpx;
	height: 88rpx;
	display: flex;
	border-bottom: 1px solid #eee;
	position: relative;
	font-size: 28rpx;
}
.input-label{
	flex: 0 0 150rpx;
	display: flex;
	justify-content: flex-start;
	align-items: center;
}
.input{
	flex: 1;
	display: flex;
	justify-content: flex-start;
	align-items: center;
}
.clear{
	position: absolute;
	width: 60rpx;
	right:0;
	top:0;
	bottom:0;
	display: flex;
	justify-content: center;
	align-items: center;
}
.close {
	position: absolute;
	top: -15rpx;
	right: -15rpx;
	width: 50rpx;
	height: 50rpx;
	z-index: 11;
	background: url('data:image/png;base64,')no-repeat center center;
	background-size: 100% 100%;
}
.placeholderCls{
	color: #999999;
	font-size: 28rpx;
	text-align: left;
}
.title{
	font-size: 30rpx;
	font-weight: bold;
}
.subtitle{
	font-size: 28rpx;
	color: #999;
	margin-top: 20rpx;
}
