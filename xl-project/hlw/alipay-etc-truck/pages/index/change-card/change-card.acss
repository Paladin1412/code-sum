.hide {
    display: none !important;
}

.info {
    height: 80rpx;
    line-height: 80rpx;
}

.wrapper {
    width: 750rpx;
    box-sizing: border-box;
    padding: 0 30rpx;
}

.apply-wrapper {
    padding: 10rpx 30rpx;
}

.extra {
    background-image: url('https://gw.alipayobjects.com/zos/rmsportal/dOfSJfWQvYdvsZiJStvg.svg');
    background-size: contain;
    background-repeat: no-repeat;
    background-position: right center;
    opacity: 0.2;
    height: 20px;
    width: 20px;
    padding-left: 10px;
}

.contact {
    background-image: url('https://gw.alipayobjects.com/zos/rmsportal/dOfSJfWQvYdvsZiJStvg.svg');
    background-size: contain;
    background-repeat: no-repeat;
    background-position: right center;
    opacity: 0.2;
    height: 20px;
    width: 20px;
    padding-left: 10px;
}

.btn-wrapper {
    margin: 50rpx 30rpx;
    color: white;
}

.title {
    margin-left: 30rpx;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 30rpx;
}

.img-list {
    flex: 0 0 233rpx;
    margin: 20rpx 0;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.img-item {
    position: relative;
    flex: 0 0 323rpx;
    height: 233rpx;
}

.close {
    position: absolute;
    top: -15rpx;
    right: -15rpx;
    width: 50rpx;
    height: 50rpx;
    z-index: 11;
    background: url('data:image/png;base64,')no-repeat center center;
    background-size: 100% 100%;
}

.title {
    width: 90%;
    display: flex;
    justify-content: center;
    padding-top: 60rpx;
    padding-bottom: 60rpx;
    border-bottom: 1rpx solid #ccc;
}