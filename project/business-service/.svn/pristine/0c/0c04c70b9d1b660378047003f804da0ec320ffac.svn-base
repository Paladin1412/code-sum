.sorry-warp {
    position: absolute;
    top: 0;
    bottom: 0;
    width: 100%;
}
.empty-icon {
    height: 10rem;
    background: url(../../resource/h5/images/common/404.png) no-repeat 50% bottom;
    background-size: 4.1rem 4.04rem;
}
.sorry-warp p {
    padding-top: 0.68rem;
    font-size: 0.62rem;
    color: #666;
    text-align: center;
    line-height: 0.68rem;
}
.sorry-warp .refresh-btn {
    margin: 1.64rem auto 0.70rem;
    width: 7.1rem;
    height: 1.78rem;
    background-color: #4c72ff;
    border: 1px solid #4c72ff;
    border-radius: 8px;
    text-align: center;
    line-height: 1.78rem;
    font-size: 0.66rem;
    color: #fff;
}

.sorry-warp .close-btn {
    margin: 0 auto;
    width: 7.1rem;
    height: 1.78rem;
    background-color: #fff;
    border: 1px solid #d8d8d8;
    border-radius: 8px;
    line-height: 1.78rem;
    font-size: 0.66rem;
    color: #666;
    text-align: center;
}
