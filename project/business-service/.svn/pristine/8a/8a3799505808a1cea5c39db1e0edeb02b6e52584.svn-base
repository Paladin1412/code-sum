@charset "utf-8";
body {
    font: normal 12px "SimSun";
}

.frame-wrap {
    position: relative;
    height: 100%;
    overflow: hidden;
}
/* 头部 */

.user-info {
    position: relative;
    width: 130px;
    height: 61px;
    background: url(../img/avatar-bg.png) 0 0;
    text-align: center;
    /*cursor: pointer;*/
}

.user-info .avatar {
    margin: 9px auto 0 auto;
    width: 26px;
    height: 26px;
    border: 1px solid #fff;
    border-radius: 100%;
    overflow: hidden;
}

.user-info .avatar img {
    width: 100%;
    height: 100%;
}

.user-info .username {
    padding: 2px;
    margin-top: 1px;
    color: #fff;
    font-size:12px;
}

.userinfo-pop {
    position: absolute;
    display: none;
    top: 5px;
    left: 90px;
    width: 220px;
    background-color: transparent;
    z-index: 1099;
    text-align: left;
}

.userinfo-pop .content {
    padding: 5px;
    border: 1px solid #ccc;
    position: relative;
    border-radius: 3px;
    box-shadow: 0 0 6px rgba(0, 0, 0, .3);
    background-color: #fff;
    font-size: 12px;
}

.userdetail {
    padding: 5px 15px;
    line-height: 26px;
    overflow: hidden;
}
.userdetail p{
    float:left;
    width: 100%;
}
.userdetail em {
    float: left;
    color: #333;
}
.userdetail span{
    float:none;
    display: block;
}

.userdetail .changePwd-button {
    line-height: 35px;
    height: 35px;
    padding-left: 55px;
    display: block;
    background: url(../img/pwd-edit.png) 0 center no-repeat;
    background-size: 35px;
    color: #5D91F3;
    cursor: pointer;
}

.userinfo-pop .arrow,
.userinfo-pop .arrow i {
    position: absolute;
    display: block;
    height: 0;
    width: 0;
    font-size: 0;
    line-height: 0;
}

.userinfo-pop .arrow {
    top: 10px;
    left: -12px;
    border-color: transparent #ccc transparent transparent;
    border-style: dashed solid dashed dashed;
    border-width: 6px;
}

.userinfo-pop .arrow i {
    top: -6px;
    left: -5px;
    border-color: transparent #fff transparent transparent;
    border-style: dashed solid dashed dashed;
    border-width: 6px;
}

.header {
    margin-left: 130px;
    height: 60px;
    border-bottom: 1px solid #e7eaec;
}

.header .logo {
    margin-left: 13px;
    width: 350px;
    height: 100%;
    background: url(../img/logo.png) 0 center no-repeat;
}
.header .logo .logo-txt{
	font-family: "方正正中黑简体 Regular";
	font-weight: bold;
  	color: #4c7fe2;
  	font-size: 19px;
  	margin-left: 40px;
    line-height: 60px;
}
.header .frame-tools {
    position: relative;
    padding-right: 5px;
    margin-top: 24px;
    color: #999;
    vertical-align: middle;
}

.skin-wrap,
.message-wrap,
.exit-wrap,
.version-wrap,
.rscj-wrap,
.plug-wrap {
    float: right;
    background: transparent;
    position:relative;
}

div.message-wrap{
	position:relative;	
}

.header .skin-button,
.header .exit-button,
.header .message-button,
.header .plug-button {
    padding-left: 21px;
    margin-right: 18px;
    background-position: 0 0;
    background-repeat: no-repeat;
    cursor: pointer;
    height: 18px;
    line-height: 18px;
}

.header .message-button {
    background-image: url(../img/index_message.png);
    background-repeat: no-repeat;
    display: block;
    height: 18px;
}

.header .plug-button {
    background-image: url(../img/index_plugin.png);
    position:relative;
}

.header .plug-button {
    color: #999999;
    display: inline-block;
    height: 36px;
}

.header .version-button:hover,
.header .message-button:hover,
.header .exit-button:hover,
.header .plug-button:hover {
    color: #5D91F3;
}

.header .message-button {
    background-image: url(../img/index_message.png);
    background-repeat: no-repeat;
}

.header .skin-button {
    background-image: url(../img/icon/skin-icon.png);
}

.header .rscj-button {
    cursor: pointer;
    padding-left: 21px;
    margin-right: 18px;
    display: block;
    height: 18px;
    line-height: 18px;
    background-image: url(../img/icon/index_rscj.png);
    background-repeat: no-repeat;
}

.header .version-button {
    cursor: pointer;
    padding-left: 21px;
    margin-right: 18px;
    display: block;
    height: 18px;
    line-height: 18px;
    background-image: url(../img/index_version.png);
    background-repeat: no-repeat;
}

.header .exit-button {
    display: block;
    margin-right: 18px;
    background-image: url(../img/icon/exit-icon.png);
    background-repeat: no-repeat;
    padding-left: 21px;
    line-height: 18px;
    height: 18px;
}

.header .message-button i {
    position: absolute;
    top: -8px;
    right: 8px;
    display: block;
    line-height: 14px;
    font-size: 12px;
    font-weight: normal;
    color: #fff;
    border-radius: 3px;
    white-space: nowrap;
    background-color: #ff0000;
    display: block;
}
/* 用户皮肤 */

.skin-pop {
    position: absolute;
    top: 24px;
    left: 127px;
    display: none;
    width: 70px;
    background-color: transparent;
    z-index: 9999;
}

.skin-pop .content {
    padding: 5px;
    border: 1px solid #ccc;
    position: relative;
    border-radius: 3px;
    box-shadow: 0 0 5px rgba(0, 0, 0, .3);
    background-color: #fff;
    font-size: .75em;
}

.skin-pop .content a,
.skin-pop .content a img,
.skin-pop .content a span {
    display: block;
    text-align: center;
}

.skin-pop .content a {
    position: relative;
    padding: 5px 0;
    border-radius: 4px;
    border: 1px solid transparent;
}

.skin-pop .content a img {
    margin: 0 auto;
}

.skin-pop .content a span {
    padding: 4px 0 0 0;
    line-height: 14px;
    color: #666;
}

.skin-pop .content a:hover {
    border-color: #ccc;
    background-color: #f2f2f2;
}

.skin-pop .content a.selected i {
    position: absolute;
    top: 26px;
    left: 34px;
    width: 16px;
    height: 16px;
    background: url(../img/skin-selected.png);
}

.skin-pop .arrow,
.skin-pop .arrow i {
    position: absolute;
    display: block;
    height: 0;
    width: 0;
    font-size: 0;
    line-height: 0;
}

.skin-pop .arrow,
{
    top: -12px;
    left: 30px;
    border-color: transparent transparent #ccc transparent;
    border-style: dashed dashed solid dashed;
    border-width: 6px;
}

.skin-pop .arrow i {
    top: -5px;
    left: -6px;
    border-color: transparent transparent #fff transparent;
    border-style: dashed dashed solid dashed;
    border-width: 6px;
}
/* 插件下载 */

.plug-pop {
    position: absolute;
    display: none;
    top: 35px;
    left: -96px;
    width: 254px;
    background-color: transparent;
    z-index: 1001;
    cursor: text;
}

.plug-pop .content {
    padding: 5px;
    border: 1px solid #ccc;
    position: relative;
    border-radius: 3px;
    box-shadow: 0 0 5px rgba(0, 0, 0, .3);
    background-color: #fff;
    /*font-size: .75em;*/
}

.plug-pop .content a,
.plug-pop .content a img,
.plug-pop .content a span {
    display: block;
    text-align: center;
}

.plug-pop .content a {
    position: relative;
    padding: 0;
    border: none;
}

.plug-pop .content a img {
    margin: 0 auto;
}

.plug-pop .content a span {
    padding: 4px 0 0 0;
    line-height: 14px;
    color: #666;
}

.plug-pop .content a:hover {
    border-color: #ccc;
    background-color: #f2f2f2;
    background: url(../img/bg_dload_btn.png) center no-repeat;
    color: #fff;
}

.plug-pop .content a.selected i {
    position: absolute;
    top: 26px;
    left: 34px;
    width: 16px;
    height: 16px;
    background: url(../img/skin-selected.png);
}

.plug-pop .arrow,
.plug-pop .arrow i {
    position: absolute;
    display: block;
    height: 0;
    width: 0;
    font-size: 0;
    line-height: 0;
}

.plug-pop .arrow {
    top: -15px;
    left: 120px;
    border-color: transparent transparent #ccc transparent;
    border-style: dashed dashed solid dashed;
    border-width: 8px;
}

.plug-pop .arrow i {
    top: -7px;
    left: -8px;
    border-color: transparent transparent #fff transparent;
    border-style: dashed dashed solid dashed;
    border-width: 8px;
}

.plug-pop .content a {
    padding: 0;
    border: none;
}

.bg-dotted {
    background: url(../img/pic_dotted.png) 0 bottom no-repeat;
}

.plugin-item {
    display: inline-block;
    /*padding:8px 0;*/
    
    width: 254px;
    vertical-align: middle;
}

.plugin-item span,
.plugin-item a {
    display: inline-block;
    /*float:left;*/
    
    margin: 0 3px;
    text-align: center;
}

.file-type {
    width: 23px;
    height: 13px;
    background: url(../img/bg_file_type.png) center no-repeat;
    color: #fff;
    font: normal 10px/13px 'Simsun';
}

td .plugin-item td .file-name {
    width: 100px;
    color: #2BB2DF;
    line-height: 16px;
    text-align: left;
    cursor: pointer;
}
/* start 添加名称hover样式 by thliu*/

.plugin-item a.file-name:hover {
    background-image: none;
    color: #D44826;
}

.plugin-item a.file-name {
    width: 106px;
    text-align: left;
}
/* end 添加名称hover样式 by thliu*/

.file-size {
    width: 58px;
    color: #A6A6A6;
}

.file-url {
    width: 35px;
    height: 18px;
    background: url(../img/bg_dload_btn.png) center no-repeat;
    color: #fff;
    font: normal 14px/18px 'SimSun';
}

.table-plugin-dload {
    table-layout: fixed;
    border-spacing: 0;
}

.table-plugin-dload td {
    padding: 5px 0;
}
/*消息通告*/

.message-pop {
    position: absolute;
    display: none;
    top: 35px;
    left: -160px;
    width: 380px;
    background-color: transparent;
    z-index: 1001;
}

.message-pop .arrow,
.message-pop .arrow i {
    position: absolute;
    display: block;
    height: 0;
    width: 0;
    font-size: 0;
    line-height: 0;
}

.message-pop .arrow {
    top: -16px;
    left: 180px;
    border-color: transparent transparent #ccc transparent;
    border-style: dashed dashed solid dashed;
    border-width: 8px;
}

.message-pop .arrow i {
    top: -7px;
    left: -8px;
    border-color: transparent transparent #fff transparent;
    border-style: dashed dashed solid dashed;
    border-width: 8px;
}

.message-pop .content {
    overflow: hidden;
    border: 1px solid #ccc;
    position: relative;
    border-radius: 3px;
    box-shadow: 0 0 5px rgba(0, 0, 0, .3);
    background-color: #fff;
    font-size: 12px;
    left: 20px;
}
/* 消息框 */

.message-header {
    padding: 6px 20px;
    height: 16px;
    line-height: 16px;
    color: #333;
    background-color: #f2fdff;
}

.message-header i,
.message-header em {
    display: inline-block;
    vertical-align: top;
}

.message-header em {
    margin-left: 3px;
}

.message-clear {
    float: right;
    color: #25b7e2;
}

.message-body {
    padding: 5px 10px 0 10px;
    overflow: hidden;
    overflow-y: auto;
    max-height: 250px;
}

.message-body li {
    padding: 8px 0;
    border-bottom: 1px solid #e5e5e5;
}

/*.message-body li:last-child {
    border: none;
}*/

.message-body dl {
    position: relative;
    *width: 320px;
}
/* 消息详细 */

.message-detail dt {
    position: absolute;
    margin-top: -11px;
    right: 0;
    top: 50%;
    width: 60px;
    height: 22px;
    line-height: 22px;
    text-align: right;
}

.message-detail dt a {
    float: right;
    padding: 2px 4px 2px 2px;
    border-radius: 3px;
    vertical-align: middle;
    border: 1px solid transparent;
    color: #25b7e2;
    cursor: pointer;
}

.message-detail dt a i {
    vertical-align: top;
}

.message-detail dt a em {
    display: inline-block;
    vertical-align: top;
}

.message-detail dt a:hover {
    border-color: #25b7e2;
    background-color: #f2fdff;
}

.message-detail dd {
    margin: 0 60px 0 0;
}

.message-detail dd h4 {
    line-height: 20px;
    font-size: 1em;
    color: #888;
}

.message-detail dd h4 em {
    margin: 0 20px 0 0;
    color: #333;
}

.message-detail dd p {
    line-height: 18px;
}

.message-detail dd a {
    /*color: #25b7e2;*/
    cursor: pointer;
}
.message-detail dd a:hover {
    color: #D44826;
}
/* 消息回复 */

.message-reply {
    margin: 10px 0 0 0;
}

.message-reply dt {
    position: absolute;
    right: 0;
    top: 0;
    width: 42px;
    height: 42px;
}

.message-reply dt a {
    display: block;
    text-align: center;
    border-radius: 3px;
    height: 16px;
    line-height: 16px;
    color: #fff;
    background-color: #d0d0d0;
}

.message-reply dt a.btn-reply {
    padding: 5px 0;
    margin: 0 0 5px 0;
    *margin: 0 0 3px 0;
    background-color: #25b7e2;
}

.message-reply dd {
    margin: 0 52px 0 0;
}

.message-reply dd textarea {
    overflow: auto;
    border: 1px solid #ccc;
    width: 260px;
    height: 40px;
}

.message-reply dd textarea:focus {
    border-color: #25b7e2;
}

.message-footer {
    text-align: center;
    background: #fff;
}

.message-footer a {
    display: block;
    padding: 5px 0 5px 0;
    color: #25b7e2;
}

.message-footer a:hover {
    background-color: #f2fdff;
}

.message-empty {
    display: none;
}

.message-empty p {
    line-height: 100px;
    text-align: center;
    font-size: 1.25em;
}

.message-pop-mask {
    display: none;
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 1000;
    top: 0px;
    left: 0px;
    overflow: hidden;
    background: transparent;
}
/* 头部 end */

.frame-body {
    position: absolute;
    top: 61px;
    bottom: 0;
    width: 100%;
}
/* 导航菜单 */
/* 导航菜单end */
/* 侧边菜单 */

.frame-menu {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    padding: 10px 0;
    width: 130px;
    background-color: #202236;
    color: #90919b;
    overflow: auto;
}

.frame-menu li {
    padding: 4px 0;
}

.frame-menu li div {
    padding: 5px 10px 10px 10px;
    height: 32px;
    vertical-align: top;
    cursor: pointer;
}

.frame-menu li div:hover {
    background-image: url(../img/menu-bg.png);
    background: rgba(0, 0, 0, .2);
    color: #fff;
}

.frame-menu li div ins,
.frame-menu li i,
.frame-menu li em {
    display: inline-block;
    height: 16px;
    line-height: 16px;
    vertical-align: top;
}

.frame-menu li div ins {
    float: right;
    width: 16px;
    height: 16px;
    /*background: url(../img/angle-top-bottom.png) no-repeat center 5px;*/
    
    opacity: .6;
    filter: alpha(opacity=60);
}

.frame-menu li div ins.bg {
    background: url(../img/angle-top-bottom.png) no-repeat center 5px;
}

.frame-menu li i {
    float: left;
    width: 16px;
    -webkit-animation-duration: .7s;
    -moz-animation-duration: .7s;
    -o-animation-duration: .7s;
    animation-duration: .7s;
    -webkit-animation-fill-mode: both;
    -moz-animation-fill-mode: both;
    -o-animation-fill-mode: both;
    animation-fill-mode: both
}

.frame-menu li i img {
    display: block;
}

.frame-menu li em {
    float: left;
    margin: 0 0 0 5px;
    width: 70px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.frame-menu li li {
    padding: 8px 10px 8px 35px;
    height: 32px;
    cursor: pointer;
    vertical-align: top;
}

.frame-menu li li em {
    width: 70px;
}

.frame-menu li li.selected,
.frame-menu li li:hover {
    background-image: url(../img/menu-bg.png);
    background: rgba(0, 0, 0, .2);
    color: #fff;
    border-left: 3px solid #1687CA;
    background-color: #646D7D;
    padding-left: 32px;
}

.frame-menu li li:hover i,
.frame-menu li div:hover i {
    -webkit-animation-name: swing;
    -moz-animation-name: swing;
    -o-animation-name: swing;
    animation-name: swing
}

.frame-menu li ins {
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(../img/angle-menu-ie.png) center 0 no-repeat;
    vertical-align: top;
}

.frame-menu li ins.close {
    background-position: center -16px;
}

.frame-menu .task-num {
    display: inline-block;
    width: 30px;
    height: 16px;
    background: url(../img/menu-bg.png);
    border-radius: 6px;
    line-height: 16px;
    font-size: 12px;
    color: #fff;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
/*end*/

.frame-content {
    position: relative;
