/*start 消息悬浮层样式*/
.message-float-layer {
    position: absolute;
    margin: -214px -331px;
    top: 100%;
    left: 100%;
    width: 331px;
    height: 214px;
    background: url(../img/icon/message-background.png) no-repeat 0 0;
    /*border: 1px solid #333;*/
    /*background-color: #FFF;*/
}
.message-close {
    display: block;
    margin: 5px 0 0 310px;
    width: 14px;
    height: 14px;
    font-size: 14px;
    cursor: pointer;
}
.message-close-alarm{
	display: block;
    margin: 170px 0 0 260px;
    width: 60px;
    height: 14px;
    font-size: 14px;
    cursor: pointer;
}
.message-item-wrap {
    position: absolute;
    top: 46%;
    margin-top: -45px;
}
.close-alarm-content div {
	font-size: 14px;
	padding: 10px 0;
	text-align:center;
}
.close-alarm-checkbox {
	height: 14px;
	line-height: 14px;
	cursor: pointer;
}
.close-alarm-checkbox .checkbox {
	padding-left:24px;
	background: url(../img/cred-checkbox.png) no-repeat 0 10px;
}
.checkbox-checked .checkbox {
	background: url(../img/cred-checkbox.png) no-repeat -24px 10px;
}
.close-alarm-content .checkbox-text{

}
.close-alarm-checkbox div{
	display:inline;
}
.message-item {
    margin: 15px 0 0 25px;
    height: 22px;
    line-height: 22px;
    cursor: pointer;
}
.message-item:hover {
    color: #44a3ec;
}
.message-content,
.message-type-icon,
.message-splice {
    display: block;
    float: left;
}
.message-content {
    font-size: 14px;
    width: 244px;
}
.message-type-icon {
    width: 22px;
    height: 22px;
}
.message-splice {
    margin: 0 10px;
    font-size: 22px;
}
.message-num {
    color: #fa3551;
}
.accept-icon {
    background: url(../img/icon/accept-icon.png) no-repeat 0 0;
}
.approve-icon {
    background: url(../img/icon/approve-icon.png) no-repeat 0 0;
}
.repair-icon {
    background: url(../img/icon/repair-icon.png) no-repeat 0 0;
}
.special-icon {
    background: url(../img/icon/special-icon.png) no-repeat 0 0;
}
/*end 消息悬浮层样式*/