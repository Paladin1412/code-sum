<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment></comment>
    <!-- 平台sso服务器地址 -->
	<entry key="SSO.BASE.ENDPOINT">https://wj.ahga.gov.cn/publicsso</entry>
	<!-- 安全过滤器仅在Link容器内生效(请求头的useragent中包含linkmessager) -->
