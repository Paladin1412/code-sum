	<!-- 平台分配给应用系统的Id -->
	<entry key="JMT.CLIENT.ID">bg</entry>
	<!-- 平台分配给应用系统的接入密钥 -->
	<entry key="JMT.CLIENT.SECRET">5523c88dd347d1b7cc617f632b7efdb7</entry>
	<!-- 不需安全认证的Url(支持正则表达式) -->
	<entry key="IGNORE.URL.PATTERN">/ssoclient*|*.css|*.js|*.png|*.jpg|*.gif|*.bmp|*.html|*.htm|/apply/receive|/file/*|/common/*|/images/*</entry>
